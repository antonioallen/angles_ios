# platform :ios, '9.0'

target 'Angles' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Angles
  pod 'PermissionScope'
  pod 'SwiftOverlays', '~> 3.0.0'
  pod 'Pager'
  pod "Pulsator"

  target 'AnglesTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'AnglesUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
